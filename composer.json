{
    "name": "rpqa99/rpsimplemenu",
    "description": "This is a simple menu to add Singles, Structures, Channels, Categories, Custom menus (with description), etc to your name menu for CRAFT CMS V3.x",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "simple rp menu"
    ],
    "support": {
        "docs": "https://github.com/rpqa99/rp-simple-menu/blob/master/README.md",
        "issues": "https://github.com/rpqa99/rp-simple-menu/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "rpqa99",
            "homepage": "https://github.com/rpqa99"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "rpqa99\\rpsimplemenu\\": "src/"
        }
    },
    "extra": {
        "name": "Simple RP Menu",
        "handle": "rp-simple-menu",
        "developer": "rpqa99",
        "developerUrl": "https://github.com/rpqa99",
        "documentationUrl": "https://github.com/rpqa99/rp-simple-menu/blob/master/README.md",
        "changelogUrl": "https://github.com/rpqa99/rp-simple-menu/blob/master/CHANGELOG.md",
        "components": {
            "simpleRpMenuService": "rpqa99\\rpsimplemenu\\services\\SimpleRpMenuService",
            "simpleRpMenuItemsService": "rpqa99\\rpsimplemenu\\services\\SimpleRpMenuItemsService"
        },
        "class": "rpqa99\\rpsimplemenu\\SimpleRpMenu"
    }
}
